{
  "name": "@webgal-agent/agent-core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/tools/index.js",
  "types": "./dist/tools/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/tools/index.d.ts",
      "default": "./dist/tools/index.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "default": "./dist/tools/index.js"
    },
    "./tools/index": {
      "types": "./dist/tools/index.d.ts",
      "default": "./dist/tools/index.js"
    },
    "./tools/diff-snapshot": {
      "types": "./dist/tools/diff-snapshot.d.ts",
      "default": "./dist/tools/diff-snapshot.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    },
    "./types/index": {
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "cli": "tsx src/cli.ts",
    "cli:help": "tsx src/cli.ts --help",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@types/node": "^20.0.0",
    "@webgal-agent/tool-bridge": "*",
    "glob": "^10.0.0",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^2.1.0",
    "@vitest/coverage-v8": "^2.1.0"
  }
}

